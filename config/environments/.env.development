# Development Environment Configuration
ENVIRONMENT=development

# Alpaca Trading (Paper Trading)
ALPACA_API_KEY=your_dev_key
ALPACA_SECRET_KEY=your_dev_secret
ALPACA_PAPER=True
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Alpha Vantage
ALPHA_VANTAGE_API_KEY=your_dev_key

# Email (Test)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=dev@example.com
SMTP_PASSWORD=your_dev_password
ALERT_EMAIL=dev@example.com

# Database (Local)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/investorbot_dev

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=logs/dev.log

# Feature Flags
ENABLE_REAL_TRADING=False
ENABLE_EMAIL_NOTIFICATIONS=True
ENABLE_MONITORING=True

# Rate Limiting
API_RATE_LIMIT=100
API_RATE_WINDOW=60

# Cache
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=300
